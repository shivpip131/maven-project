pipeline
{
agent any
stages
{
stage('scm checkout')
{
steps
{git 'https://github.com/shivpip131/maven-project.git'}
}
stage ('compile code') {
steps {
withMaven(jdk: 'jdklocal', maven: 'mavenlocal') {
    sh 'mvn compile'
}
}
}
stage ('test') {
steps {
withMaven(jdk: 'jdklocal', maven: 'mavenlocal') {
    sh 'mvn test'
    }
    }
    }



}
}
